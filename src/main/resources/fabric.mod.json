{
  "schemaVersion": 1,
  "id": "wycraft",
  "version": "${version}",
  "name": "Wycraft Tools",
  "description": "some tools",
  "authors": [
    "cdc1234"
  ],
  "contact": {
    "homepage": "https://wiki.wycraft.cn/",
    "issues": "https://github.com/cdc12345/WycraftMod/issues",
    "sources": "https://github.com/cdc12345/WycraftMod"
  },
  "license": "GPL-3.0",
  "environment": "client",
  "accessWidener": "wycraft.accesswidener",
  "entrypoints": {
    "client": [
      "org.cdc.wycraft.client.WycraftClient"
    ],
    "main": [
      "org.cdc.wycraft.Wycraft"
    ]
  },
  "mixins": [
    "wycraft.mixins.json",
    {
      "config": "wycraft.client.mixins.json",
