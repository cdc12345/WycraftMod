    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "icon": "assets/wycraft/icon.png",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.github_releases": "https://github.com/cdc12345/WycraftMod/releases"
